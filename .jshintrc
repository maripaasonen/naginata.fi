{
  "bitwise": true,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "globalstrict": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "maxcomplexity": 8,
  "maxdepth": 4,
  "maxlen": 100,
  "maxparams": 4,
  "maxstatements": 14,
  "noarg": true,
  "nonew": true,
  "onevar": false,
  "quotmark": "single",
  "strict": true,
  "trailing": true,
  "undef": true,
  "unused": true,

  "boss": false,
  "eqnull": false,
  "expr": false,
  "loopfunc": true,
  "smarttabs": false,
  "sub": true,

  "globals": {
    "browser": true,
    "console": true,
    "jQuery": true,
    "$": true,
    "node": false,
    "require": true,
    "module": true,
    "location": true,
    "document": true,
    "window": true,
    "__dirname": false,
    "process": false
