{
  "env": {
    "amd": false,
    "browser": true,
    "node": true
  },
  
  "globals": {
    "jQuery": false,
    "$": true,
    "require": true,
    "module": true,
    "location": true,
    "document": true,
    "window": true,
    "__dirname": false,
    "process": true
  },

  "rules": {
    "block-scoped-var": 0,
    "brace-style": 0,
    "camelcase": 0,
    "complexity": [0, 11],
    "consistent-this": [0, "self"],
    "curly": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "max-depth": [0, 4],
    "max-len": [0, 80, 4],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 2,
    "new-parens": 2,
    "no-alert": 1,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-comma-dangle": 2,
    "no-console": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 0,
    "no-dupe-keys": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-empty-label": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-func-assign": 2,
    "no-global-strict": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-loop-func": 2,
    "no-mixed-requires": [0, false],
    "no-multi-str": 1,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-array": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 0,
    "no-shadow": 1,
    "no-sync": 0,
    "no-ternary": 0,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-underscore-dangle": 1,
    "no-unreachable": 2,
    "no-unused-expressions": 1,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-with": 2,
    "no-wrap-func": 2,
    "one-var": 0,
    "quote-props": 0,
    "quotes": [2, "single"],
    "radix": 0,
    "regex-spaces": 2,
    "semi": 2,
    "strict": 2,
    "unnecessary-strict": 1,
    "use-isnan": 2,
    "wrap-iife": 0,
    "wrap-regex": 0
  }
}
